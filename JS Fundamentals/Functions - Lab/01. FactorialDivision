function factorialDivision(numOne, numTwo) {

    let result = factorieCalc(numOne) / factorieCalc(numTwo)
    console.log(result.toFixed(2));

    function factorieCalc(number) {

        let result = 1;
        while (number != 1) {
            result *= number;
            number--
        }
        return result;
    }
}
factorialDivision(5, 2)
